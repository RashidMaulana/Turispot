<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="50dp"
        tools:context=".KategoriMakanan">

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="1dp"
            android:background="#00FFFFFF"
            app:layout_constraintBottom_toTopOf="@+id/kAlam"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/back__1_"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/Kategori"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:onClick="fBack"
            android:padding="10dp"
            android:text="Kategori"
            android:textAlignment="center"
            android:textColor="#090101"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/kAlam"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_marginTop="80dp"
            android:backgroundTint="#208F5A"
            android:onClick="alam"
            android:shadowColor="#FFFFFF"
            android:text="Alam"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/kMakanan"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_marginTop="80dp"
            android:backgroundTint="#208F5A"
            android:shadowColor="#FFFFFF"
            android:text="KULINER"
            android:textColor="#FFFFFF"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/kSejarah"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="136dp"
            android:layout_marginTop="80dp"
            android:backgroundTint="#208F5A"
            android:onClick="Sejarah"
            android:shadowColor="#FFFFFF"
            android:text="Sejarah"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/kBudaya"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_marginStart="224dp"
            android:layout_marginTop="80dp"
            android:backgroundTint="#208F5A"
            android:onClick="Budaya"
            android:shadowColor="#FFFFFF"
            android:text="Budaya"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/MakananText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="160dp"
            android:text="KULINER"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textColorHighlight="#0C0C0C"
            android:textColorHint="#0E0000"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/kuliner_Abhayagiri"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="380dp"
            android:text="Abhayagiri"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cornerAbhayagiri"
            android:layout_width="310dp"
            android:layout_height="154dp"
            app:cardCornerRadius="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.386"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.29"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/KlAbhayagiri"
                android:layout_width="match_parent"
                android:layout_height="148dp"
                android:scaleX="1.7"
                android:scaleY="1.2"
                app:srcCompat="@drawable/kuliner_abhayagiri" />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cornerMangEngking"
            android:layout_width="310dp"
            android:layout_height="154dp"
            android:layout_marginTop="428dp"
            app:cardCornerRadius="40dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.275"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/KlMangengking"
                android:layout_width="350dp"
                android:layout_height="153dp"
                android:cropToPadding="true"
                android:scaleX="2.3"
                android:scaleY="1.7"
                app:layout_constraintCircleRadius="30dp"
                app:srcCompat="@drawable/kuliner_mang_engking" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="600dp"
            android:text="Mang Engking"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:id="@+id/corner_raminten"
            android:layout_width="310dp"
            android:layout_height="154dp"
            android:layout_marginTop="650dp"
            app:cardCornerRadius="40dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.376"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleX="1.3"
                android:scaleY="1.1"
                android:src="@drawable/kuliner_house_of_raminten" />



        </androidx.cardview.widget.CardView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="House of Raminten"
            android:layout_marginTop="834dp"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>





