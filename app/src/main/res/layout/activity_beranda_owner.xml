<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BerandaOwner">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        app:srcCompat="@drawable/unsplash_f_mvfz6_m3u" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:scaleX="0.8"
        android:scaleY="0.8"
        app:srcCompat="@drawable/katulumpa_1__1_" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/montserrat"
        android:text="Blue Lagoon"
        android:textColor="@color/white"
        android:textSize="26sp"
        android:textStyle="bold" />


    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="312dp"
        android:scaleX="0.6"
        android:scaleY="0.6"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="388dp"
        android:background="@drawable/rectangle_radius"
        android:backgroundTint="#208F5A"
        android:text="" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/rectangle"
        android:backgroundTint="#FFFFFF"
        android:text="" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF"
        android:onClick="profileOwner"
        app:srcCompat="@drawable/group_12639__1_"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="328dp"
        android:layout_marginTop="12dp"
        android:background="#FFFFFF"
        android:minWidth="48dp"
        android:minHeight="48dp"
        app:srcCompat="@drawable/settings"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/NamaPemilikHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="86dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/montserrat"
        android:text="TextView"
        android:textColor="#208F5A"
        android:textSize="22sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="460dp"
        android:background="@drawable/rectangle_radius" />

    <Button
        android:id="@+id/bReview"
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="558dp"
        android:backgroundTint="#208F5A"
        android:fontFamily="@font/montserrat"
        android:gravity="center"
        android:onClick="reviewOwner"
        android:text="Lihat Review"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="400dp"
        android:fontFamily="@font/montserrat"
        android:text="Senin"
        android:textColor="@color/white"
        android:textSize="18sp"

        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="422dp"
        android:fontFamily="@font/montserrat"
        android:text="29 November 2021"
        android:textColor="@color/white"
        android:textSize="18sp" />

    <Button
        android:id="@+id/EditWisata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="400dp"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="@font/montserrat_bold"
        android:onClick="pindahProfileOwner"
        android:text="Edit"
        android:textColor="#208F5A" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="480dp"
        android:fontFamily="@font/montserrat"
        android:text="Nama Wisata"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="512dp"
        android:fontFamily="@font/montserrat"
        android:text="Alamat Wisata"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="80dp"
        android:layout_height="52dp"
        android:layout_marginLeft="282dp"
        android:layout_marginTop="488dp"
        android:background="@drawable/rectangle_radius"
        android:backgroundTint="#208F5A"
        android:text="" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="492dp"
        android:fontFamily="@font/montserrat"
        android:text="Rating"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="516dp"
        app:srcCompat="@drawable/star" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="318dp"
        android:layout_marginTop="512dp"
        android:text="4.2"
        android:textColor="@color/white"
        android:textStyle="bold" />

</RelativeLayout>